                                        press=".onTilePress"
                                    />
                                    <Link
                                        text="Javascript"
                                        press=".onTilePress"
                                    />
                                </VBox>
                            </f:content>
                        </f:Card>
                        <f:Card
                            height="100%"
                            width="100%"
                            class="sapUiSmallMarginTop"
                        >
                            <f:content>
                                <VBox
                                    alignItems="Center"
                                    justifyContent="Center"
                                    width="100%"
                                    class="sapUiSmallMargin"
                                >
                                    <Avatar

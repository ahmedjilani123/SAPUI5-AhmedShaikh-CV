                            width="100%"
                            class="sapUiSmallMarginTop"
                        >
                            <f:content>
                                <VBox
                                    alignItems="Center"
                                    justifyContent="Center"
                                    width="100%"
                                    class="sapUiSmallMargin"
                                >
                                    <Avatar

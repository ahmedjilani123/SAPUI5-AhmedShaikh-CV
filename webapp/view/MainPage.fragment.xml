                        icon="../Images/linkedin2.png"
                        press="OpenLinkbuttonPress"
                    />
                    <Button
                        type="Critical"
                        fieldGroupIds="GitHub"
                        icon="../Images/github126.png"
                        press="OpenLinkbuttonPress"
                    />
                    <Button
                        type="Success"
                        text="CV"
                        icon="sap-icon://download"
                        press="DownloadCvPress"
                    />
                    <Switch
                        change="SWitchThemePress"
                        state="{viewModel>/isDarkMode}"

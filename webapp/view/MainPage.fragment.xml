                            />
                            <ObjectStatus
                                title="Year"
                                text="2019-2020"
                            />
                            <ObjectStatus
                                title="Percentage"
                                text="78.2"
                            />
                            <ObjectStatus
                                title="Location"
                                text="Ahmedabad"
                            />
                        </layout:VerticalLayout>
                    </obj:ObjectPageSubSection>
